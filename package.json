{
  "name": "@rd-utils/react",
  "version": "0.0.1",
  "type": "module",
  "description": "Common React utilities",
  "main": "index.js",
  "module": "index.js",
  "types": "@types/index.d.ts",
  "scripts": {
    "build": "rimraf dist && webpack --config ./cfg/webpack.prod.js",
    "build:dev": "rimraf dist && webpack --config ./cfg/webpack.dev.js",
    "test": "jest --passWithNoTests",
    "test:watch": "npm run test -- --watch",
    "tsc": "tsc --noEmit",
    "lint": "eslint \"src/**/*.+(ts|tsx)\"",
    "prettier": "prettier \"src/**/*.+(ts|tsx)\"",
    "format": "npm run prettier -- --write --ignore-unknown",
    "validate": "npm run tsc && npm run lint && npm run prettier -- --list-different && npm run test",
    "prepare": "husky"
  },
  "keywords": [
    "react",
    "utilities",
    "utils",
    "state",
    "slice",
    "registry",
    "emitter"
  ],
  "author": "Den Rostokin",
  "license": "ISC",
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ],
  "peerDependencies": {
    "@types/react": ">=16.9.0",
    "react": ">=16.9.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.3.0",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.5.11",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "husky": "^9.0.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.4.4",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^5.9.0"
  }
}
